@model IEnumerable<Department>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Department</h1>

<table class="table table-bordered table-striped" style`="width 100%">  
    <thead> 
        <tr>
            <th>    
                Department Name
            </th>
            <th>    
                Rate/Hr
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="50%">
                    @obj.Rate
                </td>
            </tr>
        }
    </tbody>
</table>





